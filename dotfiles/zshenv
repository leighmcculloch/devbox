# path for rust if it's lazily installed
export PATH=$PATH:$HOME/.cargo/bin

# path for python if it's lazily installed
export PATH=$PATH:$HOME/.local/bin

# path for yarn if it's lazily installed
export PATH=$PATH:$HOME/.yarn/bin

# path for java
export JAVA_HOME=/usr/lib/jvm/java-11-openjdk-amd64
export PATH=$PATH:$LOCAL/apache-maven-3.6.0/bin

# path for rvm
export PATH="$PATH:$HOME/.rvm/bin"

# path for lazybin scripts
export PATH=$PATH:$HOME/devel/devenv/lazybin
